{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "caseSensitive": false,
  "dictionaries": [
    "en_us",
    "en-gb",
    "misc",
    "pt-br",
    "de-de",
    "softwareTerms",
    "typescript",
    "node",
    "php",
    "html",
    "css",
    "bash",
    "fonts",
    "filetypes",
    "npm"
  ],
  "flagWords": [],
  "useGitignore": true,
  "ignorePaths": [
    "**/.git/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/vendor/**",
    "**/build/**",
    "**/.next/**",
    "**/package-lock.json",
    "**/yarn.lock",
    "**/pnpm-lock.yaml",
    "**/*.log"
  ],
  "import": [
    "~/.cache/nvim/cspell-dictionaries/node_modules/@cspell/dict-pt-br/cspell-ext.json",
    "~/.cache/nvim/cspell-dictionaries/node_modules/@cspell/dict-de-de/cspell-ext.json"
  ],
  "language": "de, de_DE, en, pt_BR",
  "version": "0.2",
  "words": [
    "Brandkit",
    "Brandkits",
    "FIXIT",
    "Formik",
    "Keychron",
    "Personio",
    "SDUI",
    "Selectables",
    "builtins",
    "codeigniter",
    "darcula",
    "disqus",
    "dont",
    "elif",
    "folke",
    "frontends",
    "gitsigns",
    "hackathon",
    "importmap",
    "intelephense",
    "interruptible",
    "keymap",
    "lockdown",
    "neotree",
    "neovim",
    "nvim",
    "nvimtools",
    "octo",
    "ollama",
    "parloa",
    "portainer",
    "shada",
    "supabase",
    "testid",
    "undercurl",
    "upsell",
    "veed",
    "veedstudio",
    "wekbit",
    "windscribe",
    "wordpress",
    "zustand"
  ]
}
